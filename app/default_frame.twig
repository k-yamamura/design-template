    <!-- Facebook and Twitter integration -->
    <meta property="og:title" content=""/>
    <meta property="og:image" content=""/>
    <meta property="og:url" content=""/>
    <meta property="og:site_name" content=""/>
    <meta property="og:description" content=""/>
    <meta name="twitter:title" content=""/>
    <meta name="twitter:image" content=""/>
    <meta name="twitter:url" content=""/>
    <meta name="twitter:card" content=""/>

    <!-- <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet"> -->
    <!-- <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,400i" rel="stylesheet"> -->

    <!-- Animate.css -->
    <link rel="stylesheet" href="{{ app.config.front_urlpath }}/assets/css/animate.css">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="{{ app.config.front_urlpath }}/assets/css/icomoon.css">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="{{ app.config.front_urlpath }}/assets/css/bootstrap.css">

    <!-- Flexslider  -->
    <link rel="stylesheet" href="{{ app.config.front_urlpath }}/assets/css/flexslider.css">

    <!-- Owl Carousel  -->
    <link rel="stylesheet" href="{{ app.config.front_urlpath }}/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="{{ app.config.front_urlpath }}/assets/css/owl.theme.default.min.css">

    <!-- Theme style  -->
    <link rel="stylesheet" href="{{ app.config.front_urlpath }}/assets/css/style.css">

    <!-- Modernizr JS -->
    <script src="{{ app.config.front_urlpath }}/assets/js/modernizr-2.6.2.min.js"></script>
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
    <script src="{{ app.config.front_urlpath }}/assets/js/respond.min.js"></script>
    <![endif]-->
    <!-- for original theme CSS -->
    {% block stylesheet %}{% endblock %}

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{ app.config.front_urlpath }}/js/vendor/jquery-1.11.3.min.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}"><\/script>')</script>

    {# ▼Head COLUMN #}
    {% if PageLayout.Head %}
        {# ▼上ナビ #}
        {{ include('block.twig', {'Blocks': PageLayout.Head}) }}
        {# ▲上ナビ #}
    {% endif %}
    {# ▲Head COLUMN #}

</head>

<body id="page_{{ app.request.get('_route') }}" class="{{ body_class|default('other_page') }}">

<div class="fh5co-loader"></div>

<div id="page">

    {# ▼HeaderInternal COLUMN #}
    {% if PageLayout.Header %}
        {# ▼上ナビ #}
        {{ include('block.twig', {'Blocks': PageLayout.Header}) }}
        {# ▲上ナビ #}
    {% endif %}
    {# ▲HeaderInternal COLUMN #}

    {# ▼TOP COLUMN #}
    {% if PageLayout.ContentsTop %}
        {{ include('block.twig', {'Blocks': PageLayout.ContentsTop}) }}
    {% endif %}
    {# ▲TOP COLUMN #}

    {# ▼LEFT COLUMN #}
    {% if PageLayout.SideLeft %}
        <div id="side_left" class="side">
            {{ include('block.twig', {'Blocks': PageLayout.SideLeft}) }}
        </div>
    {% endif %}
    {# ▲LEFT COLUMN #}

    {# ▼メイン上部 #}
    {% if PageLayout.MainTop %}
        {{ include('block.twig', {'Blocks': PageLayout.MainTop}) }}
    {% endif %}
    {# ▲メイン上部 #}

    {% block main %}{% endblock %}

    {# ▼メイン下部 #}
    {% if PageLayout.MainBottom %}
        {{ include('block.twig', {'Blocks': PageLayout.MainBottom}) }}
    {% endif %}
    {# ▲メイン下部 #}

    {# ▼RIGHT COLUMN #}
    {% if PageLayout.SideRight %}
        <div id="side_right" class="side">
            {{ include('block.twig', {'Blocks': PageLayout.SideRight}) }}
        </div>
    {% endif %}
    {# ▲RIGHT COLUMN #}

    {# ▼BOTTOM COLUMN #}
    {% if PageLayout.ContentsBottom %}
        {{ include('block.twig', {'Blocks': PageLayout.ContentsBottom}) }}
    {% endif %}
    {# ▲BOTTOM COLUMN #}

    {# ▼Footer COLUMN#}
    {% if PageLayout.Footer %}
        {{ include('block.twig', {'Blocks': PageLayout.Footer}) }}
    {% endif %}
    {# ▲Footer COLUMN#}

</div>

<div class="gototop js-top">
    <a href="#" class="js-gotop"><i class="icon-arrow-up"></i></a>
</div>

<div id="drawer" class="drawer sp"></div>

<div class="overlay"></div>


<!-- jQuery Easing -->
<script src="{{ app.config.front_urlpath }}/assets/js/jquery.easing.1.3.js"></script>
<!-- Bootstrap -->
<script src="{{ app.config.front_urlpath }}/assets/js/bootstrap.min.js"></script>
<!-- Waypoints -->
<script src="{{ app.config.front_urlpath }}/assets/js/jquery.waypoints.min.js"></script>
<!-- Carousel -->
<script src="{{ app.config.front_urlpath }}/assets/js/owl.carousel.min.js"></script>
<!-- countTo -->
<script src="{{ app.config.front_urlpath }}/assets/js/jquery.countTo.js"></script>
<!-- Flexslider -->
<script src="{{ app.config.front_urlpath }}/assets/js/jquery.flexslider-min.js"></script>
<!-- Main -->
<script src="{{ app.config.front_urlpath }}/assets/js/main.js"></script>
<script src="{{ app.config.front_urlpath }}/js/function.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}"></script>
<script src="{{ app.config.front_urlpath }}/js/eccube.js?v={{ constant('Eccube\\Common\\Constant::VERSION') }}"></script>

{% block javascript %}{% endblock %}
</body>
</html>
